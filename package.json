{
  "name": "vue-documenter",
  "version": "1.1.0",
  "description": "An automatic Vue component documenter - yes - a component that documents components",
  "author": "Ian Johnson",
  "scripts": {
    "serve": "vue-cli-service serve src/index.js",
    "build": "vue-cli-service build --target lib --name vue-documenter ./src/index.js",
    "build-dev": "vue-cli-service build --mode development --target lib --name vue-documenter ./src/index.js"
  },
  "main": "dist/vue-documenter.common.js",
  "module": "dist/vue-documenter.umd.js",
  "unpkg": "dist/vue-documenter.umd.js",
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "dependencies": {
    "core-js": "^2.6.5",
    "prismjs": "^1.17.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "css-loader": "^3.2.0",
    "node-sass": "^7.0.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.3.1",
    "vue-loader": "^15.7.1",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.39.3"
  },
  "keywords": [
    "vuejs",
    "vue",
    "documenation",
    "dynamic analysis"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/theianjohnson/vue-documenter"
  }
}
